                  </div>
                  <!-- /.card-header -->

               <div class="col-md-12" style="background-color: #191c24; color: white;">
                  <table id="example1" class="table table-bordered table-striped">
                     <thead>
                        <tr>
                           <th>Buyer Name</th>
                           <th>Buyer Mobile</th>
                           <th>Processed By</th>
                           <th>Total Amount</th>
                           <th>Date of Purchase</th>
                           
                          
                        </tr>
                     </thead>
                     <tbody>
                        <% if(sampleData.length> 0)
                           {
                              sampleData.forEach(function(data){  %>
                        

                        <tr>
                           <td><%=data.b_name%></td>
                           <td><p class="info"><b style="font-size: 13px"><%=data.b_mobile%></b></p></td>
                           <td><%=data.admin_name%></td>
                          
                            <td class="text-success"><b><%=data.amount%></b></td>
                           
                           <td><%=data.purchase_date%></td>
                         
                         
                        </tr>
                       
                        <% }); }  %>
                        
                        </tbody>
                  </table>
               </div>
            </div>
      </div>
      
